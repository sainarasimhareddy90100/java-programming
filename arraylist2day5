import java.util.ArrayList;
    public class Arraylist {
        public static void main(String[] args) {
            ArrayList<String> list = new ArrayList<>();
            list.add("ABC");
            list.add("OEF");
            list.add("GHJ");
            System.out.println(list);
            list.remove("GHJ");
            System.out.println(list);
        }
    }
