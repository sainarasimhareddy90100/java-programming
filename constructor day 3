public class Constructor {
    int a=1;
    int b=7;
    Constructor(int a,int b)
    {
        a=5;
        b=10;
    }
    void add(int a,int b) {
        int c = a + b;
        System.out.println("Sum :" + c);
    }
    public static void main(String[] args)
    {
        Constructor S=new Constructor(6,9);
        S.add(2,4);
    }
}
