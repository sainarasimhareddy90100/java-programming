public class Preetham
{
    public static void main(String[] args)
    {
        int a = 10;
        int b = 5;
        int c = 7;

        if (a > b && a > c)
        {
            System.out.println("a is greater than both b and c");
        }
        else
        {
            System.out.println("a is not greater than both b and c");
        }
    }
}
